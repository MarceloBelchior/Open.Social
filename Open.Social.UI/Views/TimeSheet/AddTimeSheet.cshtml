
<div id="modal_form_vertical" class="modal fade"  ng-controller="TimeSheetController">
    <div class="modal-dialog">
        <div class="modal-content">
            <div class="modal-header">
                <button type="button" class="close" data-dismiss="modal">&times;</button>
                <h5 class="modal-title">Cadastro Usuario</h5>
            </div>
            <form class="form-horizontal">
                <input type="hidden" ng-model="model.id" />
                <div class="modal-body">
                    <div class="form-group">
                        <label class="control-label col-sm-3">Data Entrada</label>
                        <div class="col-sm-9">
                            <input type="text" placeholder="saida" class="form-control" maxlength="10" ng-model="model.data">
                        </div>

                
                    </div>
                    <div class="form-group">
                        <label class="control-label col-sm-3">Hora Entrada</label>
                        <div class="col-sm-9">
                            <input type="text" placeholder="entrada" class="form-control" ng-model="model.horainicio">
                        </div>
                    </div>
                    <div class="form-group">
                        <label class="control-label col-sm-3">Hora Almoço</label>
                        <div class="col-sm-9">
                            <input type="text" placeholder="saida" class="form-control" maxlength="5"  ng-model="model.horainicioalmoco">

                        </div>
                    </div>
                    <div class="form-group">
                        <label class="control-label col-sm-3">Retorno Almoço</label>
                        <div class="col-sm-9">
                            <input type="text" class="form-control"  ng-model="model.horafimalmoco">

                        </div>
                    </div>
                    <div class="form-group">
                        <label class="control-label col-sm-3">Saida</label>
                        <div class="col-sm-9">
                            <input type="text" class="form-control"  ng-model="model.saida">

                        </div>
                    </div>
                    <div class="form-group">
                        <label class="control-label col-sm-3">Comentarios</label>
                        <div class="col-sm-9">
                            <input type="text" class="form-control"  ng-model="model.comment">

                        </div>
                    </div>


                </div>
                <div class="modal-footer">
                    <button type="button" class="btn btn-link" data-dismiss="modal">Fechar</button>
                    <button type="button" class="btn btn-primary" ng-click="SaveTimeSheet()">Salvar</button>
                </div>
            </form>
        </div>
    </div>
</div>